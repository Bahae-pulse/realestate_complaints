<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
    <!-- Define Complaint tags -->
        <record id="tag_new" model="realestate.complaint.tag">
            <field name="name">Newest</field>
            <field name="color">5</field>
        </record>
    
        <record id="tag_wrong" model="realestate.complaint.tag">
            <field name="name">Not valid</field>
            <field name="color">1</field>
        </record>
        
        <record id="tag_answered" model="realestate.complaint.tag">
            <field name="name">Solved</field>
            <field name="color">10</field>
        </record>

        <record id="tag_waiting" model="realestate.complaint.tag">
            <field name="name">Waiting</field>
            <field name="color">3</field>
        </record>

        <record id="tag_progress" model="realestate.complaint.tag">
            <field name="name">Progressing</field>
            <field name="color">11</field>
        </record>
    </data>
</odoo>